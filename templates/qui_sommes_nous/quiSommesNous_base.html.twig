			<div class="smallCard" data-title="Modèle pluri et transdisciplinaire" data-imgsrc="{{ asset('images/QuiSommesNous/modele/pluri.png') }}" data-activecard="true" data-link="/QuiSommesNous/transdisciplinaire"></div>

	{% endif %}

    {% endblock %}
