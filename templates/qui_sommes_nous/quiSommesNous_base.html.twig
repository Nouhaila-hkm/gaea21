				</div>
				<div class="_background"></div>
			{% endblock %}
		</header>


		{# la barre de navigation secondaire #}
		{% block sousMenu %}
			<div id='secondaryNav' data-filename="dataPourLaSousNav/itemsQuiSommesNous.json"></div>
		{% endblock %}



		{% block bouttonRetour %}
			<div id="prevArrow" data-link="/QuiSommesNous"></div>
		{% endblock %}


			 <section id="organizationDebut"> 
	{% block organization_Debut %}
				<div class="smallCard" data-title="Comité - Organe décisionnel" data-imgsrc="{{ asset('images/QuiSommesNous/organigramme/Decisionnel.png') }}" data-activecard="true" data-link="/QuiSommesNous/Organigramme/OrganeDecisionnel"></div>

				<div class="smallCard" data-title="Bureau exécutif - Organe opérationnel" data-imgsrc="{{ asset('images/QuiSommesNous/organigramme/Operationnel.png') }}" data-activecard="true" data-link="/QuiSommesNous/Organigramme/OrganeOperationnel"></div>

				<div class="smallCard" data-title="Conseil des sage - Organe consultatif" data-imgsrc="{{ asset('images/QuiSommesNous/organigramme/Consultatif.png') }}" data-activecard="true" data-link="/QuiSommesNous/Organigramme/OrganeConsultatif"></div>

        {% endblock %}
			</section>

		<main
			id="main">
			{# Block principale de la page #}
			{% block main %}
			{% endblock %}
		</main>
			

		<section id="personnes">
{% block personnes %}
	{% endblock %}
		</section>


	{% block carousel %}
		{% if '/gestionprojet' in app.request.uri %}
			<div id="WhoCarousel"></div>

		{% elseif '/analyse' in app.request.uri %}
			<div id="WhoCarousel"></div>

		{% elseif '/lean' in app.request.uri %}
			<div id="WhoCarousel"></div>

		{% elseif '/coevolution' in app.request.uri %}
			<div id="WhoCarousel"></div>

		{% elseif '/behavior' in app.request.uri %}
			<div id="WhoCarousel"></div>

		{% elseif '/multiplicateureco' in app.request.uri %}
			<div id="WhoCarousel"></div>		
		{% endif %}
	{% endblock %}



		 <section id="organizationFin"> 

	{% block organization_fin %}
	{% if '/Organigramme' in app.request.uri %}

			<div class="smallCard" data-title="Comité - Organe décisionnel" data-imgsrc="{{ asset('images/QuiSommesNous/organigramme/Decisionnel.png') }}" data-activecard="true" data-link="/QuiSommesNous/Organigramme/OrganeDecisionnel"></div>

			<div class="smallCard" data-title="Bureau exécutif - Organe opérationnel" data-imgsrc="{{ asset('images/QuiSommesNous/organigramme/Operationnel.png') }}" data-activecard="true" data-link="/QuiSommesNous/Organigramme/OrganeOperationnel"></div>

			<div class="smallCard" data-title="Conseil des sage - Organe consultatif" data-imgsrc="{{ asset('images/QuiSommesNous/organigramme/Consultatif.png') }}" data-activecard="true" data-link="/QuiSommesNous/Organigramme/OrganeConsultatif"></div>

			{% elseif '/partenaire' in app.request.uri %}

			<div class="smallCard" data-title="Comité - Organe décisionnel" data-imgsrc="{{ asset('images/QuiSommesNous/partenaire/bientot.png') }}" data-activecard="false" data-link="/QuiSommesNous/Organigramme/OrganeDecisionnel"></div>

			<div class="smallCard" data-title="Partenaires académiques / RH" data-imgsrc="{{ asset('images/QuiSommesNous/partenaire/academique.png') }}" data-activecard="true" data-link="/QuiSommesNous/partenaire"></div>

			<div class="smallCard" data-title="Autres partenaires" data-imgsrc="{{ asset('images/QuiSommesNous/partenaire/autres.png') }}" data-activecard="true" data-link="/QuiSommesNous/autrepartenaire"></div>

			{% elseif '/autrepartenaire' in app.request.uri %}

			<div class="smallCard" data-title="Comité - Organe décisionnel" data-imgsrc="{{ asset('images/QuiSommesNous/partenaire/bientot.png') }}" data-activecard="false" data-link="/QuiSommesNous/Organigramme/OrganeDecisionnel"></div>

			<div class="smallCard" data-title="Partenaires académiques / RH" data-imgsrc="{{ asset('images/QuiSommesNous/partenaire/academique.png') }}" data-activecard="true" data-link="/QuiSommesNous/partenaire"></div>

			<div class="smallCard" data-title="Autres partenaires" data-imgsrc="{{ asset('images/QuiSommesNous/partenaire/autres.png') }}" data-activecard="true" data-link="/QuiSommesNous/autrepartenaire"></div>

			{% elseif '/modele' in app.request.uri %}

			<div class="smallCard" data-title="Modèle systémique" data-imgsrc="{{ asset('images/QuiSommesNous/modele/sys.png') }}" data-activecard="true" data-link="/QuiSommesNous/modele"></div>

			<div class="smallCard" data-title="Modèle pluri et transdisciplinaire" data-imgsrc="{{ asset('images/QuiSommesNous/modele/pluri.png') }}" data-activecard="true" data-link="/QuiSommesNous/transdisciplinaire"></div>

			{% elseif '/transdisciplinaire' in app.request.uri %}

